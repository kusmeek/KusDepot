<Service>

    <record name = "Service">

        <main>

            <summary>Represents Service records in the Catalog.</summary>

        </main>

        <property name = "Application" Cref = "String?"><summary>Application.</summary></property>

        <property name = "ApplicationVersion" Cref = "String?"><summary>ApplicationVersion.</summary></property>

        <property name = "BornOn" Cref = "String?"><summary>BornOn.</summary></property>

        <property name = "ContentStreamed" Cref = "Boolean?"><summary>ContentStreamed.</summary></property>

        <property name = "DistinguishedName" Cref = "String?"><summary>DistinguishedName.</summary></property>

        <property name = "FILE" Cref = "String?"><summary>FILE.</summary></property>

        <property name = "ID" Cref = "Guid?"><summary>ID.</summary></property>

        <property name = "Modified" Cref = "String?"><summary>Modified.</summary></property>

        <property name = "Name" Cref = "String?"><summary>Name.</summary></property>

        <property name = "Notes" Cref = "String[]?"><summary>Notes.</summary></property>

        <property name = "ObjectType" Cref = "String?"><summary>ObjectType.</summary></property>

        <property name = "ServiceInterfaces" Cref = "String?"><summary>Service Interfaces.</summary></property>

        <property name = "ServiceType" Cref = "String?"><summary>Service Implementing Type.</summary></property>

        <property name = "ServiceVersion" Cref = "String?"><summary>ServiceVersion.</summary></property>

        <property name = "Size" Cref = "String?"><summary>Size.</summary></property>

        <property name = "Tags" Cref = "String[]?"><summary>Tags.</summary></property>

        <property name = "Version" Cref = "String?"><summary>Version.</summary></property>

    </record>

    <record name = "ServiceQuery">

        <main>

            <summary>Supports querying Service records in the Catalog.</summary>

        </main>

        <property name = "Application" Cref = "String?"><summary>Application.</summary></property>

        <property name = "ApplicationVersion" Cref = "String?"><summary>ApplicationVersion.</summary></property>

        <property name = "BornOn" Cref = "String?"><summary>BornOn.</summary></property>

        <property name = "ContentStreamed" Cref = "Boolean?"><summary>ContentStreamed.</summary></property>

        <property name = "DistinguishedName" Cref = "String?"><summary>DistinguishedName.</summary></property>

        <property name = "FILE" Cref = "String?"><summary>FILE.</summary></property>

        <property name = "ID" Cref = "Guid?"><summary>ID.</summary></property>

        <property name = "Modified" Cref = "String?"><summary>Modified.</summary></property>

        <property name = "Name" Cref = "String?"><summary>Name.</summary></property>

        <property name = "Notes" Cref = "String[]?"><summary>Notes.</summary></property>

        <property name = "ObjectType" Cref = "String?"><summary>ObjectType.</summary></property>

        <property name = "ServiceInterfaces" Cref = "String?"><summary>Service Interfaces.</summary></property>

        <property name = "ServiceType" Cref = "String?"><summary>Service Implementing Type.</summary></property>

        <property name = "ServiceVersion" Cref = "String?"><summary>ServiceVersion.</summary></property>

        <property name = "Size" Cref = "String?"><summary>Size.</summary></property>

        <property name = "Tags" Cref = "String[]?"><summary>Tags.</summary></property>

        <property name = "Version" Cref = "String?"><summary>Version.</summary></property>

    </record>

    <record name = "ServiceResponse">

        <main>

            <summary>Catalog service response.</summary>

        </main>

        <property name = "Services" Cref = "Service[]"><summary>Matching records.</summary></property>

    </record>

</Service>