<?xml version = "1.0" encoding = "utf-8" ?>

<DataContent>

    <class name = "DataContent">

        <main>

            <summary>DataContent.</summary>

            <remarks>The content of a single DataItem.</remarks>

        </main>

        <property name = "Binary" Cref = "Byte[]?"><summary>BinaryItem.</summary></property>

        <property name = "Code" Cref = "String?"><summary>CodeItem.</summary></property>

        <property name = "DataSet" Cref = "HashSet{DataItem}?"><summary>DataSetItem.</summary></property>

        <property name = "Generic" Cref = "List{Object}?"><summary>GenericItem.</summary></property>

        <property name = "GuidReference" Cref = "Dictionary{String,Object?}?">
            <summary>GuidReferenceItem.</summary>
            <remarks>
                Stores the content of a GuidReferenceItem as a dictionary. Keys include:
                <list type="bullet">
                    <item><term>"Content"</term><description>Content.</description></item>
                    <item><term>"Next"</term><description>The next GuidReferenceItem.</description></item>
                    <item><term>"Previous"</term><description>The previous GuidReferenceItem.</description></item>
                </list>
            </remarks>
        </property>

        <property name = "MSBuild" Cref = "Dictionary{String,Object?}?">
            <summary>MSBuildItem.</summary>
            <remarks>
                Stores the content of an MSBuildItem as a dictionary. Keys include:
                <list type="bullet">
                    <item><term>"Content"</term><description>Content.</description></item>
                    <item><term>"Requirements"</term><description>The requirements.</description></item>
                    <item><term>"Sequence"</term><description>The sequence.</description></item>
                </list>
            </remarks>
        </property>

        <property name = "MultiMedia" Cref = "Dictionary{String,Object?}?">
            <summary>MultiMediaItem.</summary>
            <remarks>
                Stores the content of an MultiMediaItem as a dictionary. Keys include:
                <list type="bullet">
                    <item><term>"Content"</term><description>Content.</description></item>
                    <item><term>"Source"</term><description>Source Stream.</description></item>
                </list>
            </remarks>
        </property>

        <property name = "Text" Cref = "String?"><summary>TextItem.</summary></property>

        <method name = "GetKnownTypes">

            <summary>DataContractSerializer Known Types.</summary>

            <returns Cref = "IEnumerable{Type}">Known Types.</returns>

        </method>

        <method name = "IParsable{DataContent}.Parse">

            <summary>Parse DataContent.</summary>

            <param name = "input" Cref = "String">Input to Parse.</param>

            <param name = "format" Cref = "IFormatProvider?">Format Provider.</param>

            <returns Cref = "DataContent">Parsed DataContent.</returns>

        </method>

        <method name = "Parse">

            <summary>Parse DataContent.</summary>

            <param name = "input" Cref = "String">Input to Parse.</param>

            <param name = "format" Cref = "IFormatProvider?">Format Provider.</param>

            <returns Cref = "DataContent?">Parsed DataContent.</returns>

        </method>

        <method name = "TryParse">

            <summary>Try to Parse DataContent.</summary>

            <param name = "input" Cref = "String">Input to Parse.</param>

            <param name = "format" Cref = "IFormatProvider?">Format Provider.</param>

            <param name = "key" Cref = "DataContent">Output DataContent.</param>

            <returns Cref = "Boolean">True if input was parsed successfully.</returns>

        </method>

    </class>

</DataContent>