<?xml version = "1.0" encoding = "utf-8" ?>

<TextItem>

    <class name = "TextItem">

        <main>

            <summary>Text with Any or No Structure.</summary>

            <remarks>This item stores human readable text.</remarks>

        </main>

        <content name = "Content" Cref = "String?">

            <summary>Item Content.</summary>

        </content>

        <field name = "Language" Cref = "String?">

            <summary>Item Language.</summary>

        </field>

        <field name = "Sync" Cref = "TextItemSync">

            <summary>Synchronization Objects.</summary>

        </field>

        <constructor name = "ParameterlessConstructor">

            <summary>Parameterless Constructor.</summary>

            <returns Cref = "TextItem">New TextItem.</returns>

        </constructor>

        <constructor name = "Constructor">

            <summary>Constructor</summary>

            <param name = "content" Cref = "String?">Item Content.</param>

            <param name = "file" Cref = "String?">File.</param>

            <param name = "id" Cref = "Guid?">Item ID.</param>

            <param name = "name" Cref = "String?">Item Name.</param>

            <param name = "notes" Cref = "IEnumerable{String}?">Item Notes.</param>

            <param name = "tags" Cref = "IEnumerable{String}?">Tags.cs.</param>

            <param name = "type" Cref = "String?">Item Type.</param>

            <param name = "language" Cref = "String?">Content Language.</param>

            <returns Cref = "TextItem">TextItem.</returns>

        </constructor>

        <method name = "op_Equality">

            <summary>Equality Operator.</summary>

            <param name = "a" Cref = "TextItem?">A TextItem.</param>

            <param name = "b" Cref = "TextItem?">Other TextItem.</param>

            <returns Cref = "Boolean">True if TextItems are equal.</returns>

        </method>

        <method name = "op_Inequality">

            <summary>Inequality Operator.</summary>

            <param name = "a" Cref = "TextItem?">A TextItem.</param>

            <param name = "b" Cref = "TextItem?">Other TextItem.</param>

            <returns Cref = "Boolean">True if TextItems are not equal.</returns>

        </method>

        <method name = "FromFile">

            <summary>Create from file.</summary>

            <param name = "path" Cref = "String">Absolute path and file name.</param>

            <returns Cref = "TextItem?">Object created from file.</returns>

        </method>

        <method name = "GetContent">

            <summary>Get Content.</summary>

            <returns Cref = "String?">Content.</returns>

        </method>

        <method name = "GetLanguage">

            <summary>Get Language.</summary>

            <returns Cref = "String?">Language.</returns>

        </method>

        <method name = "OnDeserialized">

            <summary>Serialization Support.</summary>

            <param name = "context" Cref = "StreamingContext">Unused.</param>

        </method>

        <method name = "Parse">

            <summary>Parse CodeItem.</summary>

            <param name = "input" Cref = "String">Input to Parse.</param>

            <param name = "format" Cref = "IFormatProvider?">Format Provider.</param>

            <returns Cref = "CodeItem?">Parsed CodeItem.</returns>

        </method>

        <method name = "SetContent">

            <summary>Set Content.</summary>

            <param name = "content" Cref = "String?">Content.</param>

            <param name = "managementkey" Cref = "ManagementKey?">ManagementKey.</param>

            <returns Cref = "Boolean">Returns true if Content was set successfully.</returns>

        </method>

        <method name = "SetLanguage">

            <summary>Set Language.</summary>

            <param name = "language" Cref = "String?">Language.</param>

            <returns Cref = "Boolean">Returns true if Language was set successfully.</returns>

        </method>

        <property name = "ExtensionData" Cref = "ExtensionDataObject?">

            <summary>IExtensibleDataObject.</summary>

        </property>

    </class>

</TextItem>