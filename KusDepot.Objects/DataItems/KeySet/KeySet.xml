<?xml version = "1.0" encoding = "utf-8" ?>

<KeySet>

    <class name = "KeySet">

        <main>

            <summary>Set of Security Keys.</summary>

        </main>

        <content name = "AllKeys" Cref = "HashSet{SecurityKey}?">

            <summary>All keys in the set.</summary>

        </content>

        <field name = "OwnerSecret" Cref = "Byte[]?">

            <summary>Owner Secret.</summary>

        </field>

        <field name = "Sync" Cref = "KeySetSync">

            <summary>Synchronization Objects.</summary>

        </field>

        <constructor name = "Constructor">

            <summary>Constructor.</summary>

            <param name = "keys" Cref = "IEnumerable{SecurityKey}?">Initial Keys.</param>

            <returns Cref = "KeySet">Set of Keys.</returns>

        </constructor>

        <constructor name = "ParameterlessConstructor">

            <summary>Parameterless Constructor.</summary>

            <returns Cref = "KeySet">New KeySet.</returns>

        </constructor>

        <method name = "AddKey">

            <summary>Add a key to the set.</summary>

            <param name = "key" Cref = "SecurityKey?">Key to add.</param>

            <param name = "name" Cref = "String?">Optional name for the key.</param>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <param name = "sign" Cref = "Boolean">True to require signature.</param>

            <returns Cref = "Boolean">True if the key was added.</returns>

        </method>

        <method name = "AddKeys">

            <summary>Add keys to the set.</summary>

            <param name = "keys" Cref = "IEnumerable{SecurityKey}?">Keys to add.</param>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <param name = "sign" Cref = "Boolean">True to require signature.</param>

            <returns Cref = "Boolean">True if all keys were added or present.</returns>

        </method>

        <method name = "ClearKeys">

            <summary>Clear All Key Material.</summary>

            <returns Cref = "Boolean">True if all key memory cleared.</returns>

        </method>

        <method name = "CheckOwner">

            <summary>Check ownership of KeySet.</summary>

            <param name = "managementkey" Cref = "ManagementKey?">ManagementKey.</param>

            <returns Cref = "Boolean">True if key belongs to KeySet Owner.</returns>

        </method>

        <method name = "CheckOwner_NoSync">

            <summary>Check ownership of KeySet without synchronization.</summary>

            <param name = "managementkey" Cref = "ManagementKey?">ManagementKey.</param>

            <returns Cref = "Boolean">True if key belongs to KeySet Owner.</returns>

        </method>

        <method name = "CreateOwnerKey">

            <summary>Create an OwnerKey.</summary>

            <param name = "subject" Cref = "String?">Subject of certificate key.</param>

            <returns Cref = "OwnerKey?">OwnerKey.</returns>

        </method>

        <method name = "op_Equality">

            <summary>Equality Operator.</summary>

            <param name = "a" Cref = "KeySet?">A KeySet.</param>

            <param name = "b" Cref = "KeySet?">Other KeySet.</param>

            <returns Cref = "Boolean">True if KeySets are equal.</returns>

        </method>

        <method name = "op_Inequality">

            <summary>Inequality Operator.</summary>

            <param name = "a" Cref = "KeySet?">A KeySet.</param>

            <param name = "b" Cref = "KeySet?">Other KeySet.</param>

            <returns Cref = "Boolean">True if KeySets are not equal.</returns>

        </method>

        <method name = "FromFile">

            <summary>Create from file.</summary>

            <param name = "path" Cref = "String">Absolute path and file name.</param>

            <returns Cref = "KeySet?">Object created from file.</returns>

        </method>

        <method name = "GetAllKeys">

            <summary>Retrieve all keys.</summary>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <returns Cref = "Task{Dictionary{String,SecurityKey}}?">All Keys.</returns>

        </method>

        <method name = "GetKey">

            <summary>Get a key by name.</summary>

            <param name = "name" Cref = "String?">Name of the key to get.</param>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <returns Cref = "Task{SecurityKey}?">The retrieved key.</returns>

        </method>

        <method name = "GetKnownTypes">

            <summary>DataContractSerializer Known Types.</summary>

            <returns Cref = "IEnumerable{Type}">Known Types.</returns>

        </method>

        <method name = "Initialize">

            <summary>Initialize the KeySet and its synchronization state.</summary>

            <returns Cref = "Boolean">True if initialized.</returns>

        </method>

        <method name = "OnDeserialized">

            <summary>Restore synchronization state after deserialization.</summary>

            <param name = "context" Cref = "StreamingContext">Streaming context.</param>

        </method>

        <method name = "Parse">

            <summary>Parse KeySet.</summary>

            <param name = "input" Cref = "String">Input to Parse.</param>

            <param name = "format" Cref = "IFormatProvider?">Format Provider.</param>

            <returns Cref = "KeySet?">Parsed KeySet.</returns>

        </method>

        <method name = "ReleaseOwnership">

            <summary>Release ownership of KeySet.</summary>

            <param name = "managementkey" Cref = "ManagementKey?">ManagementKey.</param>

            <returns Cref = "Boolean">True if ownership released successfully.</returns>

        </method>

        <method name = "RemoveKey">

            <summary>Remove a key from the set by name.</summary>

            <param name = "name" Cref = "String?">Name of the key to remove.</param>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <param name = "sign" Cref = "Boolean">True to require signature.</param>

            <returns Cref = "Boolean">True if the key was removed.</returns>

        </method>

        <method name = "RemoveKeys">

            <summary>Remove keys from the set.</summary>

            <param name = "keys" Cref = "IEnumerable{SecurityKey}?">Keys to remove.</param>

            <param name = "managementkey" Cref = "ManagementKey?">Management key for locked operations.</param>

            <param name = "sign" Cref = "Boolean">True to require signature.</param>

            <returns Cref = "Boolean">True if all keys were removed.</returns>

        </method>

        <method name = "TakeOwnership">

            <summary>Take ownership of KeySet.</summary>

            <param name = "managementkey" Cref = "ManagementKey?">ManagementKey.</param>

            <returns Cref = "Boolean">True if ownership taken successfully.</returns>

        </method>

        <method name = "ToString">

            <summary>Serialize KeySet to a Base64-encoded DataContract binary.</summary>

            <returns Cref = "String">Serialized KeySet.</returns>

        </method>

    </class>

</KeySet>