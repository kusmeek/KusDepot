<?xml version = "1.0" encoding = "utf-8" ?>

<IArkKeeperFactory>

    <interface name ="IArkKeeperFactory">

        <main>

            <summary>ArkKeeperFactory Interface.</summary>

            <remarks>This interface defines a method to create an ActorProxy for an ArkKeeper service.</remarks>

        </main>

        <method name = "Create">

            <summary>Creates an ActorProxy for an ArkKeeper service.</summary>

            <param name = "catalogname" Cref = "String">The catalog the ArkKeeper stores.</param>

            <returns Cref = "IArkKeeper">The ActorProxy.</returns>

        </method>

    </interface>

</IArkKeeperFactory>