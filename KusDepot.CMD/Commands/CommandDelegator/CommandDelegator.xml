        </main>

        <property name = "TargetHandle">

            <summary>Target Handle.</summary>

            <returns Cref = "String">Handle of delegatee.</returns>

        </property>

        <constructor name = "Constructor">

            <summary>Constructor.</summary>

            <param name = "targethandle" Cref = "String">Target Handle.</param>

            <returns Cref = "CommandDelegator">New CommandDelegator.</returns>

        </constructor>

    </class>

</CommandDelegator>